
set(PROTOBUF_NAMEVERSION "protobuf-3.0.0-beta-1")
set(PROTOBUF_ROOT "${KHEWIL_THIRDPARTIES_INSTALL_DIR}/${PROTOBUF_NAMEVERSION}")

if (EXISTS "${PROTOBUF_ROOT}")
	set(PROTOBUF_FOUND ON)
	set(PROTOBUF_INCLUDES "${PROTOBUF_ROOT}/inc")
	set(PROTOBUF_LIBRARIES "${PROTOBUF_ROOT}/lib/$<CONFIG>")
	set(PROTOBUF_BINARIES "${PROTOBUF_ROOT}/bin/$<CONFIG>")
	set(PROTOBUF_BINARIES "${PROTOBUF_BINARIES}" PARENT_SCOPE)
else()
	set(PROTOBUF_FOUND OFF)
endif()

message(STATUS "PROTOBUF_FOUND=${PROTOBUF_FOUND}")
message(STATUS "PROTOBUF_ROOT=${PROTOBUF_ROOT}")
message(STATUS "PROTOBUF_INCLUDES=${PROTOBUF_INCLUDES}")
message(STATUS "PROTOBUF_LIBRARIES=${PROTOBUF_LIBRARIES}")
message(STATUS "PROTOBUF_BINARIES=${PROTOBUF_BINARIES}")
